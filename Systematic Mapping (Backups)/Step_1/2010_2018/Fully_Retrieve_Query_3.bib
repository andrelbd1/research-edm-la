}

@article{khalili_analysis_2014,
	title = {Analysis of cooperation gain for adaptive networks in different communication scenarios},
	volume = {68},
	issn = {1434-8411},
	url = {https://www.sciencedirect.com/science/article/pii/S1434841114000831},
	doi = {10.1016/j.aeue.2014.03.014},
	abstract = {Abstract
Adaptive networks are well suited to perform distributed estimation task where the observations collected by nodes of a network are used to estimate a common network-wide desired parameter. Although the benefits of cooperation are clear for the networks with ideal links, in the presence of noisy links their behavior changes considerably. Thus, it is important to analyze, in this case, when and to what extent the cooperation improves the estimation performance. In this paper, we study the influence of noisy links on the effectiveness of cooperation for two important types of adaptive networks, i.e. incremental LMS adaptive network (ILMS) and diffusion LMS (DLMS). We first define the concept of cooperation gain and compute it for the ILMS and the DLMS algorithms with ideal and noisy links. We show that when the links are ideal, both ILMS and DLMS algorithms provide increased performance in comparison with the non-cooperative solution. On the other hand, in the presence of noisy links, the cooperation gain is not always influential, and based on the channel and data statistics, for some values of step-size, a non-cooperative scheme outperforms the ILMS and DLMS algorithms. We present some simulation results to clarify the discussions.},
	number = {9},
	journal = {AEU - International Journal of Electronics and Communications},
	author = {Khalili, Azam and Bazzi, Wael M. and Rastegarnia, Amir and Sanei, Saeid},
	month = sep,
	year = {2014},
	keywords = {Adaptive networks, Cooperation gain, diffusion, incremental, step-size},
	pages = {858--864}
}

@article{bermudez_stochastic_2011,
	title = {Stochastic analysis of an error power ratio scheme applied to the affine combination of two {LMS} adaptive filters},
	volume = {91},
	issn = {0165-1684},
	url = {https://www.sciencedirect.com/science/article/pii/S0165168411001848},
	doi = {10.1016/j.sigpro.2011.05.017},
	abstract = {The affine combination of two adaptive filters that simultaneously adapt on the same inputs has been actively investigated. In these structures, the filter outputs are linearly combined to yield a performance that is better than that of either filter. Various decision rules can be used to determine the time-varying parameter for combining the filter outputs. A recently proposed scheme based on the ratio of error powers of the two filters has been shown by simulation to achieve nearly optimum performance. The purpose of this paper is to present a first analysis of the statistical behavior of this error power scheme for white Gaussian inputs. Expressions are derived for the mean behavior of the combination parameter and for the adaptive weight mean-square deviation. Monte Carlo simulations show good to excellent agreement with the theoretical predictions.},
